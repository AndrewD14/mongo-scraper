<h1>articles</h1>
{{#each results}}
<p>
    <h4><a href={{url}} target="_blank">{{headlines}}</a></h4>
    <button class="btn btn-primary" type="button" data-toggle="collapse" data-target="#{{_id}}" aria-expanded="false" aria-controls="{{_id}}">
        Show news summary
    </button>
    <button class="btn btn-secondary" type="button">
        Add to Favorites
    </button>
    <button class="btn btn-info" type="button">
        View Comments
    </button>
</p>
<div class="collapse" id="{{_id}}">
  <div class="card card-body">
    {{summary}}
    <img src={{imgURL}} class="newsImg" style="width:614px;"/>
  </div>
</div>
{{/each}}
<nav aria-label="Page navigation example">
  <ul class="pagination">
    <li class="page-item"><a class="page-link" href="#">Previous</a></li>
    <li class="page-item"><a class="page-link" href="#">1</a></li>
    <li class="page-item"><a class="page-link" href="#">2</a></li>
    <li class="page-item"><a class="page-link" href="#">3</a></li>
    <li class="page-item"><a class="page-link" href="#">Next</a></li>
  </ul>
</nav>